{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "git-team",
  "title": "Git Team",
  "description": "Control git-team tool which allows you to automatically add co-authors to your commits.",
  "icon": "command-icon.png",
  "author": "technocidal",
  "license": "MIT",
  "commands": [
    {
      "name": "list",
      "title": "gteam list",
      "description": "Show all configured team members",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.50.5",
    "@raycast/utils": "^1.10.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
